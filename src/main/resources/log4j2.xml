<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE XML>
<Configuration status="DEBUG">
	<Appenders>
		<Console name="Console" target="SYSTEM_OUT"></Console>
		<File name="FlexCaptcha" fileName="flexcaptcha.log"></File>
	</Appenders>
	<Loggers>
		<Root level="debug">
			<AppenderRef ref="Console" />
			<AppenderRef ref="FlexCaptcha" />
		</Root>
	</Loggers>
</Configuration>		